@import 'style.styl'

::-moz-selection {
  background: rgb(255, 145, 0);
  color: #fff;
  text-shadow: none;
}

::selection {
  background: rgb(255, 145, 0);
  color: #fff;
  text-shadow: none;
}

body {
  background: url('data:image/gif;base64,') fixed top center #232d3a;
}

#sidebar {
  background: rgba(0, 110, 255, 0.2);
  h1 {
    &.default-static {
      color: rgb(0, 110, 255);
    }
  }
  .sub {
    a {
      background: rgba(0, 110, 255, 0.4);
      &:hover {
        background: rgba(0, 110, 255, 1);
      }
    }
  }
}

div {
  &.bannerunderlay {
    background: rgba(0, 110, 255, 0.4);
  }
}

header {
  &.banner {
    h1 {
      &:hover {
        background: rgb(0, 110, 255);
      }
    }
  }
}

#content {
  h1 {
    color: rgb(0, 110, 255);
  }
  .link-list {
    li {
      background: rgba(0, 110, 255, 0.2);
      &:hover {
        background: rgba(255, 145, 0, 0.2);
        footer {
          background: rgba(255, 145, 0, 0.2);
        }
      }
      footer {
        background: rgba(0, 110, 255, 0.2);
      }
    }
  }
  .post {
    background: rgba(0, 110, 255, 0.2);
    header {
      background: rgba(0, 110, 255, 0.2);
    }
  }
}
